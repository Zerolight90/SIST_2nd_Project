<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="screen">

    <select id="select" resultType="mybatis.vo.ScreenVO" parameterType="mybatis.vo.ScreenVO">
        SELECT * FROM screen
        WHERE sIdx = #{sIdx}
    </select>

    <select id="list" resultType="mybatis.vo.ScreenVO" parameterType="String">
        SELECT * FROM screen
        WHERE sIdx = #{sIdx}
    </select>
</mapper>