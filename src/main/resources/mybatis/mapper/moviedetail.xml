<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="movieDetail">

    <select id="getMovieById" parameterType="int" resultType="mybatis.vo.MovieVO">
        SELECT * FROM movie
        WHERE
        mIdx = #{mIdx}
    </select>

</mapper>